<%- include('../partials/head.ejs') %>

<main class="__login container-fluid">

  <h1>Inloggen</h1>

  <form class="__accountForm" method="post">
    <div class="form-group">
      <label for="mail">E-mailadres<span class="__manditory">*</span></label>
      <input type="email" class="form-control" id="mail" aria-describedby="E-mailadres" placeholder="Voer je e-mailadres in">
    </div>
    <div class="form-group">
      <label for="password">Wachtwoord<span class="__manditory">*</span></label>
      <input type="password" class="form-control" id="password" placeholder="Voer je wachtwoord in">
    </div>
    <button type="submit" class="btn btn-primary">Inloggen</button>
  </form>

</main>

<%- include('../partials/foot.ejs') %>
